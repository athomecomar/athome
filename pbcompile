#!/bin/sh
DIR=$(dirname "$0")
cd $DIR/pb

# clean_regex=\[\(.*?\}\]

protoc \
  -I . \
  -I ${GOPATH}/src \
  -I ${GOPATH}/src/github.com/envoyproxy/protoc-gen-validate \
  --go_out="plugins=grpc:../backend/users/" \
  --js_out="library=pb_user,binary:js" \
  --validate_out="lang=go:../backend/users/" \
  user.proto
