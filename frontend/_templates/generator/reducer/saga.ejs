---
to: '<% if(saga) { %>src/redux/<%= h.changeCase.lcFirst(name) %>/<%= h.changeCase.lcFirst(name) %>.saga.js%><% } %>'
---
<% const nameLower = h.changeCase.lcFirst(name) -%>

<% if(locals.example) { -%>
import { takeEvery } from 'redux-saga/effects';
import { actionsType } from './<%= nameLower %>.actions';
import onCountChange from './workers/onCountChange.worker';

export default function* <%= nameLower %>Watcher() {
  yield takeEvery(actionsType.INCREMENT, onCountChange);
  yield takeEvery(actionsType.DECREMENT, onCountChange);
}
<%}-%>
