---
to: src/redux/<%= h.changeCase.lcFirst(name) %>/<%= h.changeCase.lcFirst(name) %>.reducer.js
---
<% const nameLower = h.changeCase.lcFirst(name) -%>
/* eslint-disable operator-assignment */
/* eslint-disable no-param-reassign */
import produce from 'immer';
import { actionsType } from './<%=nameLower %>.actions';

const initialState = {
<% if(locals.example) { -%>
  count: 0,
<%}-%>
};

export default function <%=nameLower %>Reducer(state = initialState, action) {
  switch (action.type) {
<% if(!locals.example) { -%>
    //case actionsType.SOME_ACTION: {
    //  const nextState = produce(state, (draftState) => {
    //    draftState.count = draftState.count + 1;
    //  });
    //  return nextState;
    //}
<%}-%>
<% if(locals.example) { -%>
    case actionsType.INCREMENT: {
      const nextState = produce(state, (draftState) => {
        draftState.count = draftState.count + 1;
      });
      return nextState;
    }
    case actionsType.DECREMENT: {
      const nextState = produce(state, (draftState) => {
        draftState.count = draftState.count - 1;
      });
      return nextState;
    }
<%}-%>
    default:
      return state;
  }
}