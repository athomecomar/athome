import { createSelector } from 'reselect';

export const getState = (state) => state.<%= h.changeCase.lcFirst(name) %>;

<% if(locals.example) { -%>
export const getCount = createSelector(getState, (<%= h.changeCase.lcFirst(name) %>State) => (
  <%= h.changeCase.lcFirst(name) %>State.count
));
<%}-%>
